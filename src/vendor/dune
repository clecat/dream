(subdir gluten/lwt-unix
 (library
  (name dream_gluten_lwt_unix)
  (public_name dream.dream-gluten-lwt-unix)
  (libraries
   faraday-lwt-unix
   dream.dream-gluten-lwt
   lwt.unix
   (select
    ssl_io.ml
    from
    (lwt_ssl -> ssl_io.real.ml)
    (-> ssl_io.dummy.ml))
   (select
    tls_io.ml
    from
    (tls.lwt -> tls_io.real.ml)
    (-> tls_io.dummy.ml)))
  (modules dream_gluten_lwt_unix tls_io ssl_io)))



